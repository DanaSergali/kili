def say_hello(n,m):
    x = n + m
    return x
    
def say_hello2(n,m):
    y = n - m
    return y

def main():
    n = int(input("напиши пжалста братан а"))
    m = int(input("еще брат"))
    x = say_hello(n,m)
    y = say_hello2(x,m)
    print(y)

if __name__== "__main__"":
    main()
